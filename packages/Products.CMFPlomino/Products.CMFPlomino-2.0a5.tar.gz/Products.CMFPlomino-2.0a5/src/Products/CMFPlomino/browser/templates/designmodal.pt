    xmlns:metal="http://xml.zope.org/namespaces/metal"
    xmlns:i18n="http://xml.zope.org/namespaces/i18n"
    lang="en"
    metal:use-macro="context/main_template/macros/master"
    i18n:domain="plone">
<body>

<metal:content fill-slot="main">

    <h1 class="documentFirstHeading"><span class="plomino-design"><span class="icon-database" /> Database Design</span></h1>

    <div id="content-core">
      <div class="plomino-design"
          tal:attributes="data-pat-plominodesign string:treeUrl:${context/absolute_url}/designtree">
          <div class="tree"></div>
      </div>
    </div>
</metal:content>
</body>
</html>
