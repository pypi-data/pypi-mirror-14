	@$(NORMAL_UNINSTALL)
	@list='$(nobase_installed_test_PROGRAMS)'; test -n "$(installed_testdir)" || list=; \
	files=`for p in $$list; do echo "$$p"; done | \
	  sed -e 'h;s,^.*/,,;s/$(EXEEXT)$$//;$(transform)' \
	      -e 's/$$/$(EXEEXT)/' \
	      -e 'x;s,[^/]*$$,,;G;s,\n,,' \
