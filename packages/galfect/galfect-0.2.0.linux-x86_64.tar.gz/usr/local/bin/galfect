#!/usr/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'galfect==0.2.0','console_scripts','galfect'
__requires__ = 'galfect==0.2.0'
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.exit(
        load_entry_point('galfect==0.2.0', 'console_scripts', 'galfect')()
    )
