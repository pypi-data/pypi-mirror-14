#!/usr/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'toughcli==0.0.4','console_scripts','toughcli'
__requires__ = 'toughcli==0.0.4'
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.exit(
        load_entry_point('toughcli==0.0.4', 'console_scripts', 'toughcli')()
    )
