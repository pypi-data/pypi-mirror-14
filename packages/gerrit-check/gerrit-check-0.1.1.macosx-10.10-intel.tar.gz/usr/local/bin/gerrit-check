#!/usr/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'gerrit-check==0.1.1','console_scripts','gerrit-check'
__requires__ = 'gerrit-check==0.1.1'
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.exit(
        load_entry_point('gerrit-check==0.1.1', 'console_scripts', 'gerrit-check')()
    )
