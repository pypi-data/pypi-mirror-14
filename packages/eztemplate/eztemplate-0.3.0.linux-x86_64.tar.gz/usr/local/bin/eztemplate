#!/usr/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'eztemplate==0.3.0','console_scripts','eztemplate'
__requires__ = 'eztemplate==0.3.0'
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.exit(
        load_entry_point('eztemplate==0.3.0', 'console_scripts', 'eztemplate')()
    )
