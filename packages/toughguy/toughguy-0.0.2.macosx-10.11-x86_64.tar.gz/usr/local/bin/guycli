#!/usr/local/opt/python/bin/python2.7
# EASY-INSTALL-ENTRY-SCRIPT: 'toughguy==0.0.2','console_scripts','guycli'
__requires__ = 'toughguy==0.0.2'
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.exit(
        load_entry_point('toughguy==0.0.2', 'console_scripts', 'guycli')()
    )
