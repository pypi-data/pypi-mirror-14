<?xml version="1.0"?>
<data>
    <xpath expr="/form/notebook" position="inside">
        <page id="variant_measurements" string="Measurements"
            states="{'invisible': Eval('type').in_(['service'])}"  col="3">
            <label name="length"/>
            <field name="length"/>
            <field name="length_uom"/>
            <label name="height"/>
            <field name="height"/>
            <field name="height_uom"/>
            <label name="width"/>
            <field name="width"/>
            <field name="width_uom"/>
            <label name="weight"/>
            <field name="weight"/>
            <field name="weight_uom"/>
        </page>
    </xpath>
</data>
