# This file should be located at ~/.config/cliPublish/remotes.conf
# Each section is a remote, only one remote is currently supported.
# The section name is arbitrary.

[mh]
# Files are uploaded using rsync [...] local_file $rsync_path/remote_name
rsync_path = mxn@www.mheistermann.de:cliPublish/

# The URL prefix is used to determine the URL of a succesfully uploaded file.
# Your webserver should map this to the upload directory.
url_prefix = http://tmp.mheistermann.de/

