#!/usr/bin/env python2.7

try:
  activate_this = './bin/activate_this.py'
  execfile(activate_this, dict(__file__=activate_this))
except:
  pass

import click
from micescraper import rate_applicant

@click.command()
@click.argument("username")
@click.argument("password")
@click.argument("cid")
@click.argument("rating")
@click.argument("comment")
def main(username, password, cid, rating, comment):
  """
  """
  rate_applicant(cid, rating, comment, username, password)

if __name__ == '__main__':
  main()

