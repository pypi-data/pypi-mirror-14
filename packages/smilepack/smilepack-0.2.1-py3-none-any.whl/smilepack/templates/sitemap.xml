<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
  <url>
    <loc>{{ url_for('pages.index', _external=True) }}</loc>
    {% if last_modified -%}
        <lastmod>{{ last_modified.strftime('%Y-%m-%dT%H:%M:%SZ') }}</lastmod>
    {% endif -%}
    <changefreq>weekly</changefreq>
  </url>
  <url>
    <loc>{{ url_for('pages.generator', _external=True) }}</loc>
    {% if last_modified -%}
        <lastmod>{{ last_modified.strftime('%Y-%m-%dT%H:%M:%SZ') }}</lastmod>
    {% endif -%}
    <changefreq>weekly</changefreq>
  </url>
</urlset>
